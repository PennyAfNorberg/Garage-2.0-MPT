@model Garage_2._0_MPT.Models.ParkingsHouseStatusViewModel
<div class="row Gobig" style="position:center">
    <table class="align-self-center bg-info border-dark" >

        <thead>
            <tr><th colspan="4">Next available places</th></tr>
            <tr>
                @foreach (var item in Model.NextFree)
                {
                    <th>@item.Key</th>
                }
            </tr>
        </thead>
        <tbody>
            <tr>
                @foreach (var item in Model.NextFree)
                {
                    <td>@item.Value</td>
                }

            </tr>

        </tbody>


    </table>

</div>


